{
   "name": "Evernote keyboard shortcuts",
   "version": "0.2.1",
   "permissions": ["tabs", "https://www.evernote.com/*"],
   "icons": {
     "128": "images/logo-128.png",
     "48": "images/logo-48.png",
     "32": "images/logo-32.png",
     "16": "images/logo-16.png"
   },
   "content_scripts": [
            {
              "matches": ["https://evernote.com/*","https://www.evernote.com/*"],
              "css": ["content_scripts.css"],
              "js": ["jquery.min.js", "content_scripts.js"]
            }
    ]

}
